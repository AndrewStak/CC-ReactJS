{
  "name": "cc-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "dotenv": "^8.2.0",
    "npm": "^7.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "config": {
    "DB_PORT": "6088"
  },
  "scripts": {    
    "start": "SET REACT_APP_DB_PORT=%npm_package_config_DB_PORT%&react-scripts start",
    "build": "SET REACT_APP_DB_PORT=%npm_package_config_DB_PORT%&react-scripts build",
    "json-server": "json-server -w data/db.json -p %npm_package_config_DB_PORT%",
    "test": "SET REACT_APP_DB_PORT=%npm_package_config_DB_PORT%&react-scripts test",
    "eject": "SET REACT_APP_DB_PORT=%npm_package_config_DB_PORT%&react-scripts eject"    
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "json-server": "^0.16.3"
  }
